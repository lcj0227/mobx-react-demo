{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jquery": true,
    "amd": true,
    "commonjs": true
  },
  "rules": {
    "react/prop-types": 0,
    "no-empty": 0,
    "no-alert": 2
  }
}
